    .medicine-label-box .tab-box-list.west-boxs span.tag, .medicine-label-box .tab-box-list.first-box-list span.tag {
        width: 48% !important;
    }

    .diagnosisEdit .btn-boxs .btn-default {
        margin-right: 0;
    }

    .patientManage .form_date2, .patientList .form_date2 {
        width: 104px !important;
    }

}