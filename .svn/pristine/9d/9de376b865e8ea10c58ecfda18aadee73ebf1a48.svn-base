        padding: 0;
    }

    .patientDetail .row .col-md-2.col-sm-2:nth-of-type(5), .oldPtDetail .row .col-md-2.col-sm-2:nth-of-type(5) {
        margin-left: 38px !important;
    }

    .patientDetail .pt-detail > .doctors-action, .oldPtDetail .pt-detail > .doctors-action {
        width: 18% !important;
    }

    .patientDetail .pt-detail .pt-infor, .oldPtDetail .pt-detail .pt-infor {
