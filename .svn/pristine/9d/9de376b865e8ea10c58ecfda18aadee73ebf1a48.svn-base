    }

    .index #tabtwo .col-md-6:nth-child(3), .diagnosisEdit #tabtwo .col-md-6:nth-child(3) {
        margin-left: 0;
    }

    .diagnosis-label-box .tab-box-list.first-box-list span.tag {
