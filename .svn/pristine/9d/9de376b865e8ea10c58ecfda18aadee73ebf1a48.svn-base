    }

    .outp-change .col-md-8.col-sm-8 > .form-group:nth-of-type(1) input {
        width: 75% !important;
    }

    .outp-change .col-md-8.col-sm-8 > div:last-child .form-group:first-child, .outp-change .col-md-8.col-sm-8 div:last-child .form-group:first-child input {
