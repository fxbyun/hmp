    }

    .diagnose #divTags .col-sm-4.col-xs-4 button, .diagnose #divOtherTags .col-sm-4.col-xs-4 button {
        width: 46%;
        padding: 6px 8px;
    }

    .diagnose #divTags .span-tag {
        min-width: 100px;
    }

    .index .col-sm-6, .diagnosisEdit .col-sm-6 {
        width: 50% !important;
    }

    .right-show-box .main-sTitle ul li input[type="text"] {
        border-radius: 0;
    }

    .right-show-box .main-sTitle ul:nth-of-type(1) li:last-child input, .diagnosisEdit .main-sTitle ul:nth-of-type(1) li:last-child input {
        width: 10.2em !important;
    }

    .right-show-box .main-sTitle ul:nth-of-type(2) li:last-child input, .diagnosisEdit .main-sTitle ul:nth-of-type(2) li:last-child input {
        width: 15.5em !important;
    }

    .right-show-box .main-sTitle ul:nth-of-type(3) li div, .diagnosisEdit .main-sTitle ul:nth-of-type(3) li div {
        width: 26.2em !important;
    }

    .right-show-box .main-sTitle ul:nth-of-type(4) li:last-child div, .diagnosisEdit .main-sTitle ul:nth-of-type(4) li:last-child div {
        width: 10.8em !important;
    }

    .patient-label-box .tab-box-list span.tag {
        width: 141px;
    }

    .row.talk-tab .keyboard > div:first-child button {
        width: 35px;
    }

    .row.talk-tab .keyboard > div button:last-child {
        width: 55px !important;
    }

    .row.talk-tab .keyboard > div:last-child button {
