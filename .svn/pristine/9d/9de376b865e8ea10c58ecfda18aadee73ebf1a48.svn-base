/* @group page components   1024px
---------------------------------------------------*/
@media screen and (max-width: 1024px) {

    .patientManage .intro-sign.form-inline input[name="patient"], .patientList .intro-sign.form-inline input[name="patient"], .patientCon .intro-sign.form-inline input[name="patient"],
    .oldPatient .intro-sign.form-inline input[name="patient"], .oldPatientList .intro-sign.form-inline input[name="patient"] {
        width: 60%;
    }

    .patientManage .intro-sign.form-inline .row .col-md-4.col-sm-4:nth-of-type(3), .patientList .row .col-md-4.col-sm-4:nth-of-type(3),
    .patientCon .row .col-md-5.col-sm-4:nth-of-type(3), .oldPatient .row .col-md-4.col-sm-4:nth-of-type(3), .oldPatientList .row .col-md-4.col-sm-4:nth-of-type(3) {
        padding: 0;
    }

    .intro-sign.form-inline .form_date {
        width: 32% !important;
    }

    .patientManage #moreSeach .col-md-5.col-sm-5 input, .patientList #moreSeach .col-md-5.col-sm-5 input, .oldPatient #moreSeach .col-md-5.col-sm-5 input {
        width: 45% !important;
    }

